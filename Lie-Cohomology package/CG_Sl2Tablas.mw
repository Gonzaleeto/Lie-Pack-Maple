m_pr[1,1] := `*`:

for ii from 2 to dd+1 do:
   m_pr[ii,1] := c_let[ii-1]:
   m_pr[1,ii] := c_let[ii-1]:
od:

for ii from 2 to dd+1 do:
   for jj from 2 to dd+1 do:
      m_pr[ii,jj] := ev_exp(Tb[ii-1,jj-1],c_let):
   od:
od:
if ret = true then:
   return m_pr:
else:
   print(m_pr):
fi:
end proc:</Font></Text-field>
