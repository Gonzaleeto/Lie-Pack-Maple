<Group hide-output="false" labelreference="L3542" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal"><Font foreground="[255,153,0]" encoding="UTF-8"># El algebra A^(-) se define como A pero con la multiplicaci\303\263n 1/2(xy-yx)</Font>

Tabla_Alg_minus := proc(T) local a,b,c,dd,TT_m:
dd:=nops(T[1,2]):
for a from 1 to dd do:
   for b from 1 to dd do:
         TT_m[a,b] := 1/2*(T[a,b]-T[b,a]):
   od:
od:
return TT_m:
end proc:
</Text-field>
</Input>
</Group>
